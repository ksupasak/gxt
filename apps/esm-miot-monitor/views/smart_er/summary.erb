    <script>
      var t_map = <%= map.to_json.html_safe %>
      
      console.log(t_map)
      fillform=(r)=>{
          $.each(r,function(i,v){
            if($("."+i)){
              if($("."+i).hasClass("form-check")){ //checkbox
                $("."+i).each(function(index,sel){
                  if($(sel).find("label").html().trim()==v){
                    $(sel).find("[type='checkbox']").attr('checked',true)
                  }
                })
              }else if($("."+i).is('span')){ //text
                $("."+i).html(v)
              }
            }
          })
      }

      
      $(document).ready(function(){
      
          $('.lang').each(function(){
      
              val = $(this).attr('value')
      
              $(this).html(t_map[val]);
      
      
          })
          let report = <%= report.to_json.html_safe %>
          fillform(report)
      
      })
    </script>
