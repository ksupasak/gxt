<% require_relative 'lib'
  @record = record
  r = @record.data

  list = %w{ sbp pr o2sat temp rr dtx }
  rs = []
   for i in list
%><%
    if r["#{i}"] 
	
	v = r[i]
	
	if i=='sbp'
	
		v = "#{r['sbp']}/#{r['dbp']}"
		i = 'bp'	
	end	
	
	rs << "#{i.upcase} : #{v}"
		
		
%><% end %><% end %><%=rs.join(", ")%>
