<%
	
	record = this.model.find(params[:id])
	if record
        u = User.find record.user_id
        pv = Provider.find record.provider_id   
        u.destroy if u
        #pv.destroy if pv        
        record.destroy
	end
	return_url = '../Home/index'
	return_url = params[:return] if params[:return] 

%>
<%=redirect_to return_url%>