<textarea width="100%" rows="5">
<%=@dobj.to_json%>
</textarea>
<%
    datas = @dobj['data']
    d1 = datas.first
    cols = {}
    if d1
        pe = d1['physicalExams']
        if pe		
			for c in pe 			
				cols[c['physicalExamCode']] = c	
			end
        end
    end

    data2s = {}
    for d in datas
        pes = d['physicalExams']
        for pe in pes
            rtime = pe['recordTime']
            data2s[rtime] = {} unless rtime
            data2s[rtime][pe['physicalExamCode']] = pe['value']
        end
    end
%>
<table class="table table-striped">
<tr>
<th>Date</th>
<% for ci in cols.keys  

	col = cols[ci]

%>
<th><%=col['physicalExamCode']%> (<%=col['unit']%>)</th>
<% end %>
</tr>
<% for dk in data2s.keys %>
<tr>
<td><%=dk%></td>
<% for ci in cols.keys  

	col = cols[ci]

%>
<td><%=data2s[dk][col['physicalExamCode']]%> </td>
<% end %>
</tr>
<% end %>
</table>

<%=%>